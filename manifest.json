{
    "manifest_version": 2,
    "name": "webpage capture",
    "version": "0.0.0",
    "default_locale": "en",
    "description": "Simple webpage capture",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon-19.png",
            "38": "images/icon-38.png"
        },
        "default_title": "webpage capture",
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://*/"
    ]
}